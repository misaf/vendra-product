{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "misaf/vendra-product",
    "description": "The product module for the Vendra framework.",
    "type": "vendra-module",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filamentphp",
        "misaf",
        "vendra",
        "product"
    ],
    "homepage": "https://github.com/misaf/vendra-product",
    "support": {
        "issues": "https://github.com/misaf/vendra-product/issues",
        "source": "https://github.com/misaf/vendra-product"
    },
    "authors": [
        {
            "name": "Ehsan Mahmoodi",
            "email": "misaf.1990@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "filament/filament": "^4.7.0",
        "filament/spatie-laravel-media-library-plugin": "^4.7.0",
        "misaf/vendra-tenant": "^12.0",
        "misaf/vendra-user": "^12.0",
        "misaf/vendra-currency": "dev-master",
        "misaf/vendra-activity-log": "dev-master",
        "lara-zeus/spatie-translatable": "^1.0",
        "awcodes/filament-badgeable-column": "^3.0",
        "mokhosh/filament-jalali": "^5.1",
        "spatie/laravel-sluggable": "^3.7.5",
        "spatie/eloquent-sortable": "^4.5.0",
        "spatie/laravel-package-tools": "^1.92",
        "staudenmeir/laravel-adjacency-list": "^1.25",
        "cknow/laravel-money": "^8.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.9.2",
        "laravel/boost": "^2.0.6",
        "laravel/pint": "^1.27",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.3",
        "orchestra/testbench": "^10.9",
        "pestphp/pest": "^4.3.2",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-profanity": "^4.2.1",
        "pestphp/pest-plugin-type-coverage": "^4.0.3"
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "autoload": {
        "psr-4": {
            "Misaf\\VendraProduct\\": "src/",
            "Misaf\\VendraProduct\\Tests\\": "tests/",
            "Misaf\\VendraProduct\\Database\\Factories\\": "database/factories/",
            "Misaf\\VendraProduct\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Misaf\\VendraProduct\\ProductServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "options": {
                "symlink": true
            }
        }
    ]
}
